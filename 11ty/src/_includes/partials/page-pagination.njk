{% set parentNav = collections.all | eleventyNavigation(eleventyNavigation.parent) %}
{% set nextItem = null %}
{% for item in parentNav %}
  {% if item.key == eleventyNavigation.key %}
    {% set nextItem = parentNav[loop.index0 + 1] %}
  {% endif %}
{% endfor %}

<nav class="page-pagination"  aria-label="pagination">
  {% if nextItem %}
    <div class="line"></div>
     <div class="pagination-text next-page">
      <a href="{{ nextItem.url }}" >{{ nextItem.title }}<span class="visually-hidden">next page</span></a>
    </div>
  {% endif %}
</nav>